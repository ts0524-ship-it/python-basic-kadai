      "source": [
        "# 台形の寸法\n",
        "top = 10   # 上辺（cm）\n",
        "bottom = 20  # 下辺（cm）\n",
        "height = 5   # 高さ（cm）\n",
        "\n",
        "# 台形の面積計算\n",
        "area = (top + bottom) * height / 2\n",
        "\n",
        "# 結果を出力\n",
        "print(\"台形の面積は\", area, \"cm²です。\")\n"
      ]
    }
